{
  "name": "virtualchef",
  "version": "1.0.0",
  "description": "Virtual Chef is an web application that recommends user new food/ingredients",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidLuong98/VirtualChef.git"
  },
  "keywords": [
    "MERN"
  ],
  "author": "David Luong, Shania Kiat, Anthony Ibe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DavidLuong98/VirtualChef/issues"
  },
  "homepage": "https://github.com/DavidLuong98/VirtualChef#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "config": "^3.2.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.3",
    "react-scripts": "^3.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
